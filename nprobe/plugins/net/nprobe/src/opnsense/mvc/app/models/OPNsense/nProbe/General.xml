<model>
    <mount>//OPNsense/nprobe/general</mount>
    <description>nprobe configuration</description>
    <version>0.0.1</version>
    <items>
        <enabled type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </enabled>
        <interface type="InterfaceField">
            <Required>Y</Required>
            <Multiple>N</Multiple>
        </interface>
        <zmqendpoint type="TextField">
            <Required>Y</Required>
            <default>tcp://127.0.0.1:5556</default>
        </zmqendpoint>
        <zmqprobemode type="BooleanField">
            <default>1</default>
            <Required>Y</Required>
        </zmqprobemode>
        <ipsmode type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </ipsmode>
        <ips_events type="BooleanField">
            <default>0</default>
            <Required>N</Required>
        </ips_events>
        <zmq_ips_endpoint type="TextField">
            <Required>Y</Required>
            <default>tcp://127.0.0.1:5557</default>
        </zmq_ips_endpoint>
        <ipsconf type="TextField">
            <Required>N</Required>
        </ipsconf>
    </items>
</model>
